                    <li class="nav-item"><a href="/voituretracer" class="nav-link">Positions</a></li>

                </ul>
            </div>
        </div>
    </nav>
    <!-- END nav -->

    <section class="ftco-section ftco-cart">
        <div class="container">
            <form action="/save" method="post" id="survey-form" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="nom">Matricule:</label>
                    <input type="text" class="form-control" id="matricule" name="matricule" required>
                </div>
                <div class="form-group">
                    <label for="latitude">Type:</label>
                    <input type="text" class="form-control" id="type" name="type" required>
                </div>
                <div class="form-group">
                    <label for="longitude">Marque:</label>
                    <input type="text" class="form-control" id="marque" name="marque" required>
                </div>
                <div class="form-group">
                    <label for="ville">DateC:</label>
                    <input type="date" class="form-control" id="datec" name="datec" required>
                </div>
                <div class="form-group">
                    <label for="ville">photo:</label>
                    <input type="file" class="form-control" id="image" name="image">
                </div>

                <button type="submit" class="btn btn-primary">Ajouter</button>
            </form>
        </div>
    </section>





    <!-- loader -->
