        #survey-form {
            margin: 0px auto 15px auto;
            width: 800px;
            padding: 50px;
            border-radius: 5px;
            background-color: rgba(238, 238, 238, 0.8);
            display: block;
            align-self: center;
            align-content: center;
            text-align: center;
        }
